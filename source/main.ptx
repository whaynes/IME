               <sidebyside width="48%" xml:id="pilot_generator">
                  <figure xml:id="exciter_rotor"><!-- DV951E001_MAIN ALTERNATOR_OPERATION MANUAL_PMG  -->
                     <caption> Exciter Rotor showing Permanent Magnets</caption>
                     <image width="100%" source="pics/electric-propulsion/exciter_rotor.png"/>
                  </figure>
